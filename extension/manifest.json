{
  "name": "Pomodoro Timer",
  "version": "1",
  "description": "A simple pomodoro to control tasks.",
  "permissions": ["alarms", "notifications", "storage"],
  "browser_action": {
    "default_icon": {
      "16": "images/16x16.png",
      "32": "images/64x64.png",
      "128": "images/128x128.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/16x16.png",
    "32": "images/64x64.png",
    "128": "images/128x128.png"
  },
  "manifest_version": 2
}